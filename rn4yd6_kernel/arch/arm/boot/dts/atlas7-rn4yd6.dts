			atlas7_codec: atlas7_codec@10E30000 {
				#sound-dai-cells = <0>;
				compatible = "sirf,atlas7-codec";
				reg = <0x10E30000 0x400>;
				clocks = <&audioclk 0>;
				ldo-supply = <&ldo0>, <&ldo1>;
				status = "okay";
			};

			audioclk: clock-controller@10E30000 {
