			usp3: usp@11001000 {
				compatible = "sirf,atlas7-bt-usp",
					   "sirf,prima2-usp-pcm";
				cell-index = <3>;
				#sound-dai-cells = <0>;
				reg = <0x11001000 0x1000>;
