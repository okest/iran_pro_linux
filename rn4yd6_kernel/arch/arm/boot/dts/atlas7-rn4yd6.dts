			};

			uart6: uart@11000000 {
				cell-index = <6>;
				compatible = "sirf,atlas7-uart";
				reg = <0x11000000 0x1000>;
				interrupts = <0 100 0>;
				clocks = <&car 137>;
				clock-names = "uart";
